#include <iostream>
#include <fstream>
#include <string>

using namespace std;


void netl(string filename)
{
	ifstream netlist;
	int cells, connections, rows, columns;
	netlist.open(filename);
	
	netlist >> cells;
	netlist >> connections;
	netlist >> rows;
	netlist >> columns;
	cout << cells << "\t" << connections << "\t" << rows << "\t" << columns;
	netlist.close();
}

int main()
{
	netl("test.txt");
	return 0;
}



